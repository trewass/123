{
  "functions": {
    "app/api/pdf/[filename]/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/pdf/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/documents/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/pdf/:filename",
      "destination": "/api/pdf/[filename]"
    }
  ]
} 